{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/blog/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "Backend/index.js"
    }
  ]
}